{
  "name": "angular-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "analyze": "webpack-bundle-analyzer dist/angular-template/stats.json",
    "start": "ng serve --prod",
    "start:docker": "ng serve --configuration=docker",
    "start:staging": "ng serve --configuration=staging",
    "start:test": "ng serve --configuration=test",
    "build": "ng build --prod",
    "build:docker": "ng build --configuration=docker",
    "build:staging": "ng build --configuration=staging",
    "build:test": "ng build --configuration=test",
    "test": "ng test --watch false",
    "test:watch": "ng test",
    "stylelint": "stylelint \"src/**/*.scss\" \"!src/vendor/**/*.scss\" --syntax scss --color",
    "stylelint:fix": "stylelint \"src/**/*.scss\" \"!src/vendor/**/*.scss\" --syntax scss --fix --color",
    "lint": "eslint -c .eslintrc.json --ext .ts src/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.json --ext .ts src/**/*.ts --fix",
    "e2e": "ng e2e"
  },
  "dependencies": {
