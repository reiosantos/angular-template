    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ngneat/until-destroy": "^7.3.2",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@sentry/browser": "^5.20.0",
    "hammerjs": "^2.0.8",
